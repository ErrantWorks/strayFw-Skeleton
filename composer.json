{
    "name": "rocknroot/stray-fw-skeleton",
    "license": "BSD-3-Clause",
    "type": "project",
    "description": "Default skeleton for a strayFw new project.",
    "keywords": [ "framework", "strayFw" ],
    "autoload": {
        "psr-0": {}
    },
    "require": {
        "php": ">=7.2",
        "rocknroot/stray-fw": "~0.4"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "symfony/var-dumper": "^3.2"
    },
    "scripts": {
        "post-install-cmd": [
            "cp vendor/rocknroot/stray-fw/cli.php cli.php",
            "cp vendor/rocknroot/stray-fw/index.php web/index.php"
        ]
    },
    "minimum-stability": "beta"
}
